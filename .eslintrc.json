{
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "env": {
    "commonjs": true,
    "node": true
  },
  "plugins": ["prettier"],
  "rules": {
    "no-console": 0,
    "no-unused-vars": "warn",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "printWidth": 100,
        "semi": true,
        "tabWidth": 4,
        "trailingComma": "es5"
      }
    ]
  }
}
